def choice_sort(list):
  N = len(A) # присваиваем переменной длину листа
  for pos in range(0, N - 1): # цикл который будет находить минимальный элементы в еще неотсортированной части массива
    for k in range(pos + 1, N): # цикл который ставит следующий минимальный элемент на его место
      if A[k] < A[pos]: # проверка условия
        A[k], A[pos] = A[pos], A[k] # меняем местами два элемента массива, если условие выполнилось
  return list # возвращаем отсортированный список
